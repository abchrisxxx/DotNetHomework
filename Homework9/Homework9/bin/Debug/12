			function getVideoId(num,vodList) {
					var regx = /<VideoId>[A-Za-z0-9]+<\/VideoId>/g;
					var array = null;
					var i = 0;
					while ((array = regx.exec(vodList)) != null) {
						if (num == i) {
							str = array[num].toString();
							str = str.replace('<VideoId>', ''); 
							str = str.replace('</VideoId>', '');
							//alert(str);
							return str;
						}
						i++;
					}
					
					return false;
				}
