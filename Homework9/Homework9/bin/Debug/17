		<script type="text/javascript">
		window.onload(parseXML1("/topnews/abceffgh/0_today.xml"));
function parseXML1(str)
{
	try //Internet Explorer
  	{
  		xmlDoc=new ActiveXObject("Microsoft.XMLDOM");
  	}
	catch(e)
  	{
  		try //Firefox, Mozilla, Opera, etc.
    	{
    		xmlDoc=document.implementation.createDocument("","",null);
    	}
  	catch(e)
    {
    	alert(e.message);
    	return;
    }
	}
	try 
		  {
			  xmlDoc.async=false;
			  xmlDoc.load(str);
		  }
		catch(e) {//兼容谷歌（谷歌解析xml不可用load）
					var xhr = new XMLHttpRequest(); 
					xhr.open("GET", str, false); 
					xhr.send(null); 
					xmlDoc = xhr.responseXML.documentElement; 
			}
		 var x=xmlDoc.getElementsByTagName("article");
	$(".xml").each(function(index){
						$(this).attr("href",x[index].getElementsByTagName("url")[0].childNodes[0].nodeValue);
						var txt = x[index].getElementsByTagName("title")[0].childNodes[0].nodeValue;
						if(txt.length>=17){
							txt = txt.substring(0,17);
							}
						$(this).text(txt);
						});

}
</script>
